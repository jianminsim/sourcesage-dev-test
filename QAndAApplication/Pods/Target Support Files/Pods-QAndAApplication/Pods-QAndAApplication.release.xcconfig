GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AFMInfoBanner" "${PODS_ROOT}/Headers/Public/CVKHierarchySearcher" "${PODS_ROOT}/Headers/Public/Firebase" "${PODS_ROOT}/Headers/Public/Mantle" "${PODS_ROOT}/Headers/Public/ReactiveCocoa"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Firebase"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AFMInfoBanner" -isystem "${PODS_ROOT}/Headers/Public/CVKHierarchySearcher" -isystem "${PODS_ROOT}/Headers/Public/Firebase" -isystem "${PODS_ROOT}/Headers/Public/Mantle" -isystem "${PODS_ROOT}/Headers/Public/ReactiveCocoa"
OTHER_LDFLAGS = $(inherited) -ObjC -l"AFMInfoBanner" -l"CVKHierarchySearcher" -l"Firebase" -l"FirebaseStatic" -l"Mantle" -l"ReactiveCocoa" -l"c++" -l"icucore" -framework "CFNetwork" -framework "Foundation" -framework "Security" -framework "SystemConfiguration" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods